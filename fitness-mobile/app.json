{
  "expo": {
    "name": "gainsai",
    "slug": "gainsai",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gainsai",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thegainslab.gainsai",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to scan barcodes and take photos of food for nutrition tracking.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to select photos for nutrition tracking and support messages.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thegainslab.gainsai"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "targetSdkVersion": 35
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "We need access to your camera to scan barcodes and take photos of food for nutrition tracking."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#FE5858",
          "sounds": []
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#FE5858",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New message from your coach"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f0075d1b-fbc7-4ed0-bbe3-0ecf3298c081"
      }
    },
    "owner": "thegainslab"
  }
}
